<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <altairis.binaryStore>
    <defaultProvider name="FileSystemProvider" xdt:Transform="RemoveAttributes(name)" xdt:Locator="Match(name)" />
    <providers>
      <add name="FileSystemProvider"
           xdt:Transform="Remove" xdt:Locator="Match(name)" />
    </providers>
  </altairis.binaryStore>
</configuration>